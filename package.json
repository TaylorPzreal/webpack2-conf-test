{
  "name": "webpack2-conf-test",
  "version": "1.0.0",
  "description": "webpack2在项目中的配置实践",
  "repository": "git@github.com:TaylorPzreal/webpack2-conf-test.git",
  "author": "TaylorPzreal <taylorpzreal@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --port 3000 --progress --profile --colors",
    "start": "yarn run dev -- --hot",
    "start:dash": "webpack-dashboard -- cross-env NODE_ENV=development webpack-dev-server --config webpack.config.js --hot --profile",
    "start:browser": "yarn run start -- --open",
    "build": "rimraf dist && cross-env NODE_ENV=production webpack --config webpack.config.js --progress --profile --colors --json > stats.json",
    "localserver": "http-server ./ -p 3000"
  },
  "dependencies": {
    "angular": "^1.6.4",
    "angular-ui-router": "^1.0.3",
    "bootstrap": "^3.3.7",
    "es6-promise": "^4.1.0",
    "font-awesome": "^4.7.0",
    "jquery": "^3.2.1",
    "lodash": "^4.17.4"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-env": "^1.5.1",
    "babel-preset-es2015": "^6.24.1",
    "compression-webpack-plugin": "^0.4.0",
    "cross-env": "^5.0.0",
    "css-loader": "^0.28.4",
    "eslint": "^3.19.0",
    "eslint-loader": "^1.7.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.28.0",
    "lodash-webpack-plugin": "^0.11.4",
    "node-sass": "^4.5.3",
    "null-loader": "^0.1.1",
    "os": "^0.1.1",
    "postcss-loader": "^2.0.5",
    "precss": "^1.4.0",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.1",
    "sass-lint": "^1.10.2",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.18.1",
    "uglify-js": "^3.0.15",
    "uglifyjs-webpack-plugin": "^0.4.3",
    "url-loader": "^0.5.8",
    "webpack": "^2.6.1",
    "webpack-bundle-analyzer": "^2.8.2",
    "webpack-dashboard": "^0.4.0",
    "webpack-dev-server": "^2.4.5",
    "webpack-manifest-plugin": "^1.1.0",
    "webpack-merge": "^4.1.0",
    "webpack-parallel-uglify-plugin": "^0.4.1"
  }
}
